<h1><%= @titulo %></h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Tipo</th>
      <th>Propiedad</th>
      <th>Opciones</th>
    </tr>
  </thead>
  <tbody>
  <% @profiles.each do |profile| %>
    <tr>
      <td><%= profile.nombre %></td>
      <td><%= profile.descripcion %></td>
      <td><%= profile.type.name %></td>
      <td><%= profile.property.name %></td>
      <td>
        <%= link_to_show(admin_profile_path(profile)) %>
        <%= link_to_edit(admin_profile_path(profile)) %>
        <%= link_to_delete(admin_profile_path(profile)) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
