    <% @profile.photos.each do |image| %>
      <%= link_to image_tag(image.photo.url(:thumb)) , 
      image.photo.url(:original),
      :class=>'picture',
      :rel=>'picperfil',
      :title=>image.photo_descripcion %>
    <% end %>
<div class="page-header">
  <h1><%= @profile.nombre %></h1>
</div>
<div class= "well">
  <strong>Nombre :</strong>
  <%= @profile.nombre %></br>
  <strong>Descripcion :</strong>
  <%= @profile.descripcion %></br>
  <strong>Tipo de Inmueble</strong>
  <%= @profile.type.name%></br>
  <strong>Propiedad</strong>
  <%= @profile.property.name %></br>
</div>
